@{
    ViewData["Title"] = "Car Page";
}

@*@model List<IntegerWebApplication.Models.Car>*@
@model Car[]

<div>
    <h1>@ViewData["Title"]</h1>
    <table class="table-bordered table table-striped">
        <thead>
            <tr>
                <th>ID Registration</th>
                <th>Type</th>
                <th>Merek</th>
                <th>Varian</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in ViewBag.Cars)
            {
                <tr>
                    <td>@item.IDRegistration</td>
                    <td>@item.Type</td>
                    <td>@item.Merek</td>
                    <td>@item.varian</td>
                </tr>
            }
        </tbody>
    </table>
</div>